{{#each beer}}
<div>
  <img src="{{this.image_url}}" alt="">
  <p>{{this.name}}</p>
  <br>
  <p>{{this.tagline}}</p>
  <br>
  <p>{{this.description}}</p> 
  <br>
  <p>Food Pairing</p>
  <ul>
    {{#each this.food_pairing}}
    <li>{{this}}</li>
    {{/each}}
  </ul>
  <br>
  <p>Brewers Tips</p>
  <p>{{this.brewers_tips}}</p>
</div>
{{/each}}

